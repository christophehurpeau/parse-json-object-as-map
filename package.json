{
  "name": "parse-json-object-as-map",
  "version": "1.1.0",
  "description": "JSON.parse but returns Map instead of Object",
  "keywords": ["JSON.parse", "map"],
  "author": "Christophe Hurpeau <christophe@hurpeau.com> (http://christophe.hurpeau.com/)",
  "license": "ISC",
  "repository": "git@github.com:christophehurpeau/parse-json-object-as-map.git",
  "homepage": "https://github.com/christophehurpeau/parse-json-object-as-map#readme",
  "bugs": {
    "url": "https://github.com/christophehurpeau/parse-json-object-as-map/issues"
  },
  "engines": {
    "node": ">=6.5.0"
  },
  "main": "./lib/index.js",
  "typings": "./lib/index.d.ts",
  "scripts": {
    "clean": "rm -Rf docs dist",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "generate:test-coverage":
      "rm -Rf docs/coverage/ ; NODE_ENV=production BABEL_ENV=test jest --coverage --coverageReporters=lcov --coverageDirectory=docs/coverage/",
    "lint": "eslint lib/",
    "postcheckout": "yarnhook",
    "postmerge": "yarnhook",
    "postrewrite": "yarnhook",
    "precommit": "lint-staged",
    "preversion": "yarn run lint && pob-repository-check-clean",
    "release": "pob-repository-check-clean && pob-release",
    "test": "jest",
    "version": "pob-version"
  },
  "lint-staged": {
    "{package.json,.eslintrc.json}": ["prettier --write", "git add"],
    "lib/**/*.json": ["prettier --write", "git add"],
    "lib/**/*.js": ["eslint --fix --quiet", "git add"]
  },
  "prettier": {
    "trailingComma": "es5",
    "singleQuote": true,
    "printWidth": 100
  },
  "commitlint": {
    "extends": ["@commitlint/config-conventional"]
  },
  "jest": {
    "cacheDirectory": "./node_modules/.cache/jest",
    "testMatch": ["<rootDir>/lib/**/__tests__/**/*.js", "<rootDir>/lib/**/*.test.js"],
    "collectCoverageFrom": ["lib/**/*.js"],
    "moduleFileExtensions": ["js"]
  },
  "devDependencies": {
    "@commitlint/cli": "^6.2.0",
    "@commitlint/config-conventional": "^6.1.3",
    "@types/jest": "^22.2.3",
    "eslint": "^4.19.1",
    "eslint-config-pob": "^19.1.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-node": "^6.0.0",
    "eslint-plugin-prettier": "^2.6.0",
    "husky": "^0.14.3",
    "jest": "^22.4.3",
    "jest-junit-reporter": "^1.1.0",
    "lint-staged": "^7.0.5",
    "pob-release": "^4.1.1",
    "prettier": "^1.12.1",
    "yarnhook": "^0.2.0"
  }
}
